{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue",
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#9F2C2D",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/inde",
        "iconPath": "static/icons/home.png",
        "selectedIconPath": "static/icons/home.png"
      },
      {
        "text": "分类",
        "pagePath": "pages/cate",
        "iconPath": "static/icons/cate.png",
        "selectedIconPath": "static/icons/cate.png"
      },
      {
        "text": "资讯",
        "pagePath": "pages/info",
        "iconPath": "static/icons/info.png",
        "selectedIconPath": "static/icons/info.png"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/init",
      "style": {
        "navigationBarTitleText": "广州湾线上历史分享",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/inde",
      "style": {
        "navigationBarTitleText": "广州湾线上历史分享",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/cate",
      "style": {
        "navigationBarTitleText": "分类",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/info",
      "style": {
        "navigationBarTitleText": "关于",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/search",
      "style": {
        "navigationBarTitleText": "搜索"
      }
    },
    {
      "path": "pages/detail",
      "style": {
        "navigationBarTitleText": "详请"
      }
    },
    {
      "path": "pages/webview",
      "style": {
        "navigationBarTitleText": ""
      }
    },
    {
      "path": "pages/game",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "拼图"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "广州湾线上历史分享",
    "navigationBarBackgroundColor": "#8c2a2d",
    "backgroundColor": "#F8F8F8"
  }
}